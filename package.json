{
  "name": "eslint-plugin-firebase-ai-logic",
  "version": "1.10.0",
  "description": "ESLint plugin for Firebase AI Logic best practices, detecting deprecated imports, models, and common anti-patterns",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "firebase",
    "firebase-ai-logic",
    "gemini",
    "ai",
    "google-ai",
    "vertex-ai"
  ],
  "author": "Matheus Pimenta (Koda AI Studio) <studio.kodaai@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Just-mpm/eslint-plugin-firebase-ai-logic"
  },
  "homepage": "https://github.com/Just-mpm/eslint-plugin-firebase-ai-logic#readme",
  "bugs": {
    "url": "https://github.com/Just-mpm/eslint-plugin-firebase-ai-logic/issues"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/estree": "^1.0.6",
    "@types/node": "^22.15.21",
    "@typescript-eslint/rule-tester": "^8.33.0",
    "eslint": "^9.28.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.3"
  }
}